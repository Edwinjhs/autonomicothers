M=[8179 8026 6975 7801 2929 8102 3527 8778 2625 4200 2402 2852 5627 4429 7953 5701 4875 8850 7278 7652 5254 9004 5854] ;

a_0=0;
a_1=1;
for j=1:length(M)
    B=a_1;
    A=a_0+a_1;
    i=2;
    while mod(A,M(j))~=0
        C=A;
        A=A+B;
        B=C;
        i=i+1;
        if A>M(j)
            A=mod(A,M(j));
        end
    end
    disp(i)
end
